        <rdfs:comment>person that make up the different characters</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/miriam/ontologies/2022/3/SeriesOntology#Nature -->

    <owl:Class rdf:about="http://www.semanticweb.org/miriam/ontologies/2022/3/SeriesOntology#Nature">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/miriam/ontologies/2022/3/SeriesOntology#Outdoors"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/miriam/ontologies/2022/3/SeriesOntology#Number_Of_Episodes -->

    <owl:Class rdf:about="http://www.semanticweb.org/miriam/ontologies/2022/3/SeriesOntology#Number_Of_Episodes">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/miriam/ontologies/2022/3/SeriesOntology#Number_Of_Seasons"/>
