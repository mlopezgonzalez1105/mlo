    <!-- http://www.semanticweb.org/miriam/ontologies/2022/3/SeriesOntology#has_visualization_platform -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/miriam/ontologies/2022/3/SeriesOntology#has_visualization_platform">
