        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
    </owl:DatatypeProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.semanticweb.org/miriam/ontologies/2022/3/SeriesOntology#Action -->

    <owl:Class rdf:about="http://www.semanticweb.org/miriam/ontologies/2022/3/SeriesOntology#Action">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/miriam/ontologies/2022/3/SeriesOntology#Genre"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/miriam/ontologies/2022/3/SeriesOntology#has_staff"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/miriam/ontologies/2022/3/SeriesOntology#Risc_Specialist"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>the main story context has scenes wich contains speed, violence...</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/miriam/ontologies/2022/3/SeriesOntology#Actors -->

    <owl:Class rdf:about="http://www.semanticweb.org/miriam/ontologies/2022/3/SeriesOntology#Actors">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/miriam/ontologies/2022/3/SeriesOntology#Serie_Staff"/>
        <rdfs:comment>the differents characters that appear in the scenes of the serie</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/miriam/ontologies/2022/3/SeriesOntology#Building -->

    <owl:Class rdf:about="http://www.semanticweb.org/miriam/ontologies/2022/3/SeriesOntology#Building">
