    </owl:Class>
    


    <!-- http://www.semanticweb.org/miriam/ontologies/2022/3/SeriesOntology#Cameraman -->

    <owl:Class rdf:about="http://www.semanticweb.org/miriam/ontologies/2022/3/SeriesOntology#Cameraman">
