    <!-- http://www.semanticweb.org/miriam/ontologies/2022/3/SeriesOntology#has_location -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/miriam/ontologies/2022/3/SeriesOntology#has_location">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/miriam/ontologies/2022/3/SeriesOntology#Serie"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/miriam/ontologies/2022/3/SeriesOntology#Location"/>
