    <!-- http://www.semanticweb.org/miriam/ontologies/2022/3/SeriesOntology#Tv_series -->

    <owl:Class rdf:about="http://www.semanticweb.org/miriam/ontologies/2022/3/SeriesOntology#Tv_series">
