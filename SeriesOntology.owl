        <owl:disjointWith rdf:resource="http://www.semanticweb.org/miriam/ontologies/2022/3/SeriesOntology#Hospital"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/miriam/ontologies/2022/3/SeriesOntology#Comedy -->

    <owl:Class rdf:about="http://www.semanticweb.org/miriam/ontologies/2022/3/SeriesOntology#Comedy">
