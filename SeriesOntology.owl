        <rdfs:comment>theme</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/miriam/ontologies/2022/3/SeriesOntology#Hospital -->

    <owl:Class rdf:about="http://www.semanticweb.org/miriam/ontologies/2022/3/SeriesOntology#Hospital">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/miriam/ontologies/2022/3/SeriesOntology#Indoors"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/miriam/ontologies/2022/3/SeriesOntology#Indoors -->

    <owl:Class rdf:about="http://www.semanticweb.org/miriam/ontologies/2022/3/SeriesOntology#Indoors">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/miriam/ontologies/2022/3/SeriesOntology#Location"/>
        <rdfs:comment>scenes shot in a building (different rooms)</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/miriam/ontologies/2022/3/SeriesOntology#Internet_Series -->

    <owl:Class rdf:about="http://www.semanticweb.org/miriam/ontologies/2022/3/SeriesOntology#Internet_Series">
