    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.semanticweb.org/miriam/ontologies/2022/3/SeriesOntology#has_id -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/miriam/ontologies/2022/3/SeriesOntology#has_id">
