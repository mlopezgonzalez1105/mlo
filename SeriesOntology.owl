        <rdfs:sameAs>https://dbpedia.org/page/Category:Television_shows_filmed_in_Spain</rdfs:sameAs>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/miriam/ontologies/2022/3/SeriesOntology#Crime -->

    <owl:Class rdf:about="http://www.semanticweb.org/miriam/ontologies/2022/3/SeriesOntology#Crime">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/miriam/ontologies/2022/3/SeriesOntology#Genre"/>
        <rdfs:comment>the story contains a murder and a police investigation</rdfs:comment>
        <rdfs:sameAs>https://dbpedia.org/page/Criminal:_Spain</rdfs:sameAs>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/miriam/ontologies/2022/3/SeriesOntology#Director -->

    <owl:Class rdf:about="http://www.semanticweb.org/miriam/ontologies/2022/3/SeriesOntology#Director">
