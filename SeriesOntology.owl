    </owl:Class>
    


    <!-- http://www.semanticweb.org/miriam/ontologies/2022/3/SeriesOntology#Visual_Effects -->

    <owl:Class rdf:about="http://www.semanticweb.org/miriam/ontologies/2022/3/SeriesOntology#Visual_Effects">
