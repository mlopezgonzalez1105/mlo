        <owl:disjointWith rdf:resource="http://www.semanticweb.org/miriam/ontologies/2022/3/SeriesOntology#Tv_series"/>
        <rdfs:comment>series that you only can see in internet platforms like HBO, Netflix..</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/miriam/ontologies/2022/3/SeriesOntology#Lead_Actor -->

    <owl:Class rdf:about="http://www.semanticweb.org/miriam/ontologies/2022/3/SeriesOntology#Lead_Actor">
