        <rdfs:domain rdf:resource="http://www.semanticweb.org/miriam/ontologies/2022/3/SeriesOntology#Serie"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/miriam/ontologies/2022/3/SeriesOntology#Serie_Staff"/>
