    <!-- http://www.semanticweb.org/miriam/ontologies/2022/3/SeriesOntology#Visualization_Platform -->

    <owl:Class rdf:about="http://www.semanticweb.org/miriam/ontologies/2022/3/SeriesOntology#Visualization_Platform">
