    </owl:Class>
    


    <!-- http://www.semanticweb.org/miriam/ontologies/2022/3/SeriesOntology#Location -->

    <owl:Class rdf:about="http://www.semanticweb.org/miriam/ontologies/2022/3/SeriesOntology#Location">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/miriam/ontologies/2022/3/SeriesOntology#Serie"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/miriam/ontologies/2022/3/SeriesOntology#Love -->

    <owl:Class rdf:about="http://www.semanticweb.org/miriam/ontologies/2022/3/SeriesOntology#Love">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/miriam/ontologies/2022/3/SeriesOntology#Genre"/>
        <rdfs:comment>the story talks about the relationship between different characters, a story of love</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/miriam/ontologies/2022/3/SeriesOntology#Makeup_Artist -->

    <owl:Class rdf:about="http://www.semanticweb.org/miriam/ontologies/2022/3/SeriesOntology#Makeup_Artist">
