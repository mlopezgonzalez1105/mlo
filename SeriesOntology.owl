        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/miriam/ontologies/2022/3/SeriesOntology#has_name -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/miriam/ontologies/2022/3/SeriesOntology#has_name">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/miriam/ontologies/2022/3/SeriesOntology#Serie"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/miriam/ontologies/2022/3/SeriesOntology#is_published -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/miriam/ontologies/2022/3/SeriesOntology#is_published">
